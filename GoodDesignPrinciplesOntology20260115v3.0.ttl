###  https://www.ramsprinciplesofgooddesign.com/GDPO0000045
gdpo:GDPO0000045 rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf cco:ont00001808 ;
                 rdfs:domain gdpo:GDPO0000044 ;
                 rdfs:range <http://purl.obolibrary.org/obo/BFO_0000040> ;
                 rdfs:label "is about evaluated artifact"@en ;
                 skos:definition "Relates a DesignEvaluation to the material entity (artifact/system) that is the subject of the assessment."@en ;
                 skos:editorialNote "Prefer a single primary target per record; use multiple evaluations if assessing several artifacts."@en ;
                 skos:prefLabel "is about evaluated artifact"@en ;
                 skos:scopeNote "Use alongside against_principle for principled conformance."@en .


###  https://www.ramsprinciplesofgooddesign.com/GDPO0000046
gdpo:GDPO0000046 rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf cco:ont00001808 ,
                                    gdpo:GDPO0000059 ;
                 rdfs:domain gdpo:GDPO0000044 ;
                 rdfs:range gdpo:GDPO0000003 ;
                 rdfs:label "against principle"@en ;
                 skos:definition "Relates a Design Evaluation to the design principle that provides the normative evaluation criterion assessed in the evaluation record (i.e., the principle whose prescriptive content is being evaluated for compliance)."@en ;
                 skos:editorialNote "Multiple values allowed (e.g., minimalism + durability)."@en ;
                 skos:prefLabel "against principle"@en ;
                 skos:scopeNote "Enables per-principle scoring/aggregation."@en .


###  https://www.ramsprinciplesofgooddesign.com/GDPO0000048
gdpo:GDPO0000048 rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf cco:ont00001808 ;
                 owl:inverseOf :BPESO0000445 ;
                 rdfs:domain gdpo:GDPO0000044 ;
                 rdfs:range gdpo:GDPO0000052 ;
                 rdfs:label "is about using method"@en ;
                 skos:definition "Relates a DesignEvaluation to the method specification used (e.g., rubric, protocol, checklist)."@en ;
                 skos:editorialNote "If you prefer not to use IAO:plan specification, mint a local ICE subtype"@en ;
                 skos:prefLabel "is about using method"@en ;
                 skos:scopeNote "Method linkage enables reproducibility and audit."@en .


###  https://www.ramsprinciplesofgooddesign.com/GDPO0000051
gdpo:GDPO0000051 rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf <http://purl.obolibrary.org/obo/BFO_0000057> ;
                 rdfs:domain gdpo:GDPO0000055 ;
                 rdfs:range cco:ont00001017 ;
                 rdfs:label "evaluation carried out by"@en ;
                 skos:definition "Relates a design evaluation process to the agent who conducts that process."@en ;
                 skos:editorialNote "This property specializes BFO:has participant for the specific role of the conducting agent in a design evaluation process; do not use it with the ICE class 'design evaluation'."@en ;
                 skos:prefLabel "evaluation carried out by"@en ;
                 skos:scopeNote "Use only for the occurrent (process). For attribution of the resulting design evaluation record (ICE), use a record-level property such as 'authored by' (record → agent)."@en .


###  https://www.ramsprinciplesofgooddesign.com/GDPO0000054
gdpo:GDPO0000054 rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf cco:ont00001808 ;
                 rdfs:domain gdpo:GDPO0000052 ;
                 rdfs:range gdpo:GDPO0000003 ;
                 rdfs:label "operationalizes principle"@en ;
                 skos:definition "Links an evaluation method to the design principle(s) it measures."@en ;
                 skos:prefLabel "operationalizes principle"@en .


###  https://www.ramsprinciplesofgooddesign.com/GDPO0000059
gdpo:GDPO0000059 rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf cco:ont00001808 ;
                 rdfs:domain gdpo:GDPO0000044 ;
                 rdfs:range gdpo:GDPO0000003 ;
                 owl:propertyChainAxiom ( gdpo:GDPO0000048
                                          gdpo:GDPO0000054
                                        ) ;
                 rdfs:label "method-implied principle"@en ;
                 skos:definition "Relates a Design Evaluation to a Design Principle that is inferred to be relevant based on the evaluation being about using an Evaluation Method Specification that operationalizes that principle. This relation supports traceability and may over-generate if methods are repurposed."@en .


###  https://www.ramsprinciplesofgooddesign.com/GDPO0000060
gdpo:GDPO0000060 rdf:type owl:ObjectProperty ;
                 rdfs:domain gdpo:GDPO0000035 ;
                 rdfs:range gdpo:GDPO0000003 ;
                 rdfs:label "has principle component"@en ;
                 skos:definition "Relates a principle specification (a directive ICE) to a design principle statement (a directive ICE) that is included as one of the specification's component principle statements. This is a GDPO-specific structural inclusion relation and does not assert BFO continuant mereology for ICEs."@en .


###  https://www.ramsprinciplesofgooddesign.com/GDPO0000062
gdpo:GDPO0000062 rdf:type owl:ObjectProperty ;
                 rdfs:domain gdpo:GDPO0000003 ;
                 rdfs:range gdpo:GDPO0000061 ;
                 rdfs:label "has prescription component"@en ;
                 skos:definition "Relates a Design Principle to a Design Process Prescription that is included as a component directive specifying what processes are prescribed and under what (possibly temporal) conditions."@en ;
                 skos:prefLabel "has prescription component"@en .


###  https://www.ramsprinciplesofgooddesign.com/GDPO0000063
gdpo:GDPO0000063 rdf:type owl:ObjectProperty ;
                 rdfs:domain gdpo:GDPO0000061 ;
                 rdfs:range gdpo:GDPO0000019 ;
                 rdfs:label "applies during lifecycle stage"@en ;
                 skos:definition "Relates a design process prescription to a design lifecycle process stage (or lifecycle process type) during which the prescription is intended to apply. This provides coarse temporal qualification without committing to calendar time intervals."@en ;
                 skos:prefLabel "applies during lifecycle stage"@en .


###  https://www.ramsprinciplesofgooddesign.com/GDPO0000064
gdpo:GDPO0000064 rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf cco:ont00001808 ;
                 owl:propertyChainAxiom ( gdpo:GDPO0000062
                                          cco:ont00001942
                                        ) ;
                 rdfs:label "prescribes via prescription component"@en ;
                 skos:definition "A derived relation indicating that a design principle is about a process type via one of its prescription components (which itself cco:prescribes that process type)."@en ;
                 skos:editorialNote "This property is intentionally not asserted as a subproperty of cco:prescribes (ont00001942) because doing so, together with the property chain, introduces a non-regular role hierarchy that causes OWL 2 DL reasoners such as HermiT to fail (cyclic dependency involving ont00001942). Use the path: principle → has prescription component → cco:prescribes → process type for prescriptive semantics."@en ;
                 skos:prefLabel "prescribes via prescription component"@en .


###  https://www.ramsprinciplesofgooddesign.com/GoodDesignPrinciplesOntology20250818/BPESO0000445
:BPESO0000445 rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf cco:ont00001801 ;
              rdfs:label "used in evaluation"@en ;
              skos:definition "Relates an evaluation method specification to the evaluations that have used it."@en .


###  https://www.ramsprinciplesofgooddesign.com/GoodDesignPrinciplesOntology20250818/BPESO0000450
:BPESO0000450 rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf cco:ont00001808 ;
              rdfs:domain gdpo:GDPO0000044 ;
              rdfs:range :BPESO0000446 ;
              rdfs:label "is about communication content"@en ;
              skos:definition "Relates a design evaluation record to the communication content (manual/label/ad content) being assessed."@en ;
              skos:prefLabel "is about communication content"@en .


#################################################################
#    Data properties
#################################################################

###  https://www.ramsprinciplesofgooddesign.com/GDPO0000047
gdpo:GDPO0000047 rdf:type owl:DatatypeProperty ;
                 rdfs:domain gdpo:GDPO0000044 ;
                 rdfs:range xsd:decimal ;
                 rdfs:label "has score"@en ;
                 skos:definition "Records a numeric score assigned within the evaluation context."@en ;
                 skos:editorialNote "Use one data point per principle if scoring is per-criterion; otherwise one overall score. Add unit/scale via annotations (e.g., 0–1, 0–5, %)."@en ;
                 skos:prefLabel "has score"@en ;
                 skos:scopeNote "Optional; qualitative evaluations may omit."@en .


###  https://www.ramsprinciplesofgooddesign.com/GDPO0000050
gdpo:GDPO0000050 rdf:type owl:DatatypeProperty ;
                 rdfs:subPropertyOf owl:topDataProperty ;
                 rdfs:domain gdpo:GDPO0000044 ;
                 rdfs:range xsd:dateTime ;
                 rdfs:label "assessed on"@en ;
                 skos:definition "Records the date/time at which the evaluation was finalized."@en ;
                 skos:editorialNote "If you need temporal intervals, add start/end as separate properties or model the process explicitly."@en ;
                 skos:prefLabel "assessed on"@en ;
                 skos:scopeNote "Use ISO 8601 xsd:dateTime."@en .


