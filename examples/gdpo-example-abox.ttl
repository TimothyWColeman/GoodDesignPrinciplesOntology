@prefix ex: <http://example.org/gdpo-demo/> .
@prefix gdpo: <https://www.ramsprinciplesofgooddesign.com/> .
@prefix bfo: <http://purl.obolibrary.org/obo/> .
@prefix cco: <https://www.commoncoreontologies.org/> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

# =============================================================================
# GDPO Example ABox (Minimal Instantiation)
# Version: 4.0.3
# Date: 2026-01-20
#
# This file provides the minimal ABox instantiation from Appendix A1 of:
#   Coleman, T. W. (2026). Principles Are Not Properties: A BFO-Grounded
#   Ontology for Normative Design Guidance and Evaluation Traceability.
#
# NOTE ON PREFIXES: The base IRIs used below for gdpo: and cco: match the
# namespaces used in the released GDPO ontology distribution. The local
# identifiers (e.g., GDPO0000044) match the identifiers used in the paper.
# =============================================================================


# -----------------------------------------------------------------------------
# Two evaluated artifacts (material entities)
# -----------------------------------------------------------------------------

ex:BraunSK4_001 a bfo:BFO_0000040 ;
    rdfs:label "Braun SK4"@en .

ex:iPhone15_001 a bfo:BFO_0000040 ;
    rdfs:label "iPhone 15"@en .


# -----------------------------------------------------------------------------
# Grounding for canonical Rams principle statements (material-bearer provenance)
# -----------------------------------------------------------------------------

ex:RamsTenPrinciplesSource_001 a bfo:BFO_0000040 ;
    rdfs:label "Vits≈ì webpage: Ten principles for good design (material bearer)"@en .

gdpo:GDPO0000066 gdpo:GDPO0000464 ex:RamsTenPrinciplesSource_001 .
gdpo:GDPO0000067 gdpo:GDPO0000464 ex:RamsTenPrinciplesSource_001 .
gdpo:GDPO0000068 gdpo:GDPO0000464 ex:RamsTenPrinciplesSource_001 .
gdpo:GDPO0000069 gdpo:GDPO0000464 ex:RamsTenPrinciplesSource_001 .
gdpo:GDPO0000070 gdpo:GDPO0000464 ex:RamsTenPrinciplesSource_001 .
gdpo:GDPO0000071 gdpo:GDPO0000464 ex:RamsTenPrinciplesSource_001 .
gdpo:GDPO0000072 gdpo:GDPO0000464 ex:RamsTenPrinciplesSource_001 .
gdpo:GDPO0000073 gdpo:GDPO0000464 ex:RamsTenPrinciplesSource_001 .
gdpo:GDPO0000074 gdpo:GDPO0000464 ex:RamsTenPrinciplesSource_001 .
gdpo:GDPO0000075 gdpo:GDPO0000464 ex:RamsTenPrinciplesSource_001 .


# -----------------------------------------------------------------------------
# Method specifications that operationalize principle tokens
# -----------------------------------------------------------------------------

ex:DurabilityRubric_v1 a gdpo:GDPO0000052 ;
    rdfs:label "Durability + environmental impact rubric v1"@en ;
    gdpo:GDPO0000054 gdpo:GDPO0000072 ,
                     gdpo:GDPO0000074 .

ex:ManualHonestyChecklist_v1 a gdpo:GDPO0000052 ;
    rdfs:label "Manual honesty checklist v1"@en ;
    gdpo:GDPO0000054 gdpo:GDPO0000071 .


# -----------------------------------------------------------------------------
# Score scale specification (reified)
# -----------------------------------------------------------------------------

ex:Scale_0to1_Normalized a gdpo:GDPO0000461 ;
    rdfs:label "Normalized 0--1 scale (higher is better)"@en .


# -----------------------------------------------------------------------------
# Temporal regions (assessment instants)
# -----------------------------------------------------------------------------

ex:T_2024_06_15 a bfo:BFO_0000203 ;
    rdfs:label "2024-06-15T00:00:00Z"@en .

ex:T_2024_06_16 a bfo:BFO_0000203 ;
    rdfs:label "2024-06-16T00:00:00Z"@en .


# -----------------------------------------------------------------------------
# Evaluation score components (per criterion principle)
# -----------------------------------------------------------------------------

ex:Score_SK4_Durability_001 a gdpo:GDPO0000456 ;
    rdfs:label "SK4 durability score (0--1)"@en ;
    gdpo:GDPO0000458 gdpo:GDPO0000072 ;
    gdpo:GDPO0000460 ex:Scale_0to1_Normalized ;
    gdpo:GDPO0000459 "0.98"^^xsd:decimal .

ex:Score_iPhone15_Durability_001 a gdpo:GDPO0000456 ;
    rdfs:label "iPhone 15 durability score (0--1)"@en ;
    gdpo:GDPO0000458 gdpo:GDPO0000072 ;
    gdpo:GDPO0000460 ex:Scale_0to1_Normalized ;
    gdpo:GDPO0000459 "0.65"^^xsd:decimal .

ex:Score_SK4_ManualHonesty_001 a gdpo:GDPO0000456 ;
    rdfs:label "SK4 manual honesty score (0--1)"@en ;
    gdpo:GDPO0000458 gdpo:GDPO0000071 ;
    gdpo:GDPO0000460 ex:Scale_0to1_Normalized ;
    gdpo:GDPO0000459 "0.93"^^xsd:decimal .


# -----------------------------------------------------------------------------
# Evaluation records (information content entities)
# -----------------------------------------------------------------------------

ex:EvalReport_SK4_Durability_Bearer_001 a bfo:BFO_0000040 ;
    rdfs:label "SK4 durability evaluation report (PDF file)"@en .

ex:Eval_SK4_Durability_2024 a gdpo:GDPO0000044 ;
    rdfs:label "SK4 durability evaluation record (2024-06-15)"@en ;
    gdpo:GDPO0000045 ex:BraunSK4_001 ;
    gdpo:GDPO0000046 gdpo:GDPO0000072 ;
    gdpo:GDPO0000048 ex:DurabilityRubric_v1 ;
    gdpo:GDPO0000468 ex:T_2024_06_15 ;
    gdpo:GDPO0000050 "2024-06-15T00:00:00Z"^^xsd:dateTime ;
    gdpo:GDPO0000457 ex:Score_SK4_Durability_001 ;
    gdpo:GDPO0000464 ex:EvalReport_SK4_Durability_Bearer_001 .

ex:Eval_iPhone15_Durability_2024 a gdpo:GDPO0000044 ;
    rdfs:label "iPhone 15 durability evaluation record (2024-06-15)"@en ;
    gdpo:GDPO0000045 ex:iPhone15_001 ;
    gdpo:GDPO0000046 gdpo:GDPO0000072 ;
    gdpo:GDPO0000048 ex:DurabilityRubric_v1 ;
    gdpo:GDPO0000468 ex:T_2024_06_15 ;
    gdpo:GDPO0000050 "2024-06-15T00:00:00Z"^^xsd:dateTime ;
    gdpo:GDPO0000457 ex:Score_iPhone15_Durability_001 .


# -----------------------------------------------------------------------------
# Communication content and a communicative honesty evaluation
# -----------------------------------------------------------------------------

ex:SK4_UserManual_v1 a gdpo:GDPO0000447 ;
    rdfs:label "SK4 user manual (content)"@en .

ex:Eval_SK4_ManualHonesty_2024 a gdpo:GDPO0000044 ;
    rdfs:label "SK4 manual honesty evaluation record (2024-06-16)"@en ;
    gdpo:GDPO0000045 ex:BraunSK4_001 ;
    gdpo:GDPO0000450 ex:SK4_UserManual_v1 ;
    gdpo:GDPO0000048 ex:ManualHonestyChecklist_v1 ;
    gdpo:GDPO0000468 ex:T_2024_06_16 ;
    gdpo:GDPO0000050 "2024-06-16T00:00:00Z"^^xsd:dateTime ;
    gdpo:GDPO0000046 gdpo:GDPO0000071 ;
    gdpo:GDPO0000457 ex:Score_SK4_ManualHonesty_001 .


# -----------------------------------------------------------------------------
# Optional provenance via evaluation processes (process-to-record link)
# -----------------------------------------------------------------------------

ex:DrJaneSmith a cco:ont00001017 ;
    rdfs:label "Dr. Jane Smith"@en .

ex:EvalProc_SK4_Durability_2024 a gdpo:GDPO0000055 ;
    gdpo:GDPO0000463 ex:Eval_SK4_Durability_2024 ;
    gdpo:GDPO0000465 ex:BraunSK4_001 ;
    gdpo:GDPO0000051 ex:DrJaneSmith ;
    bfo:BFO_0000199 ex:T_2024_06_15 .

ex:EvalProc_SK4_ManualHonesty_2024 a gdpo:GDPO0000055 ;
    gdpo:GDPO0000463 ex:Eval_SK4_ManualHonesty_2024 ;
    gdpo:GDPO0000465 ex:BraunSK4_001 ;
    gdpo:GDPO0000051 ex:DrJaneSmith ;
    bfo:BFO_0000199 ex:T_2024_06_16 .


# -----------------------------------------------------------------------------
# Optional illustration of dispositional realization (inherence + realized in)
# -----------------------------------------------------------------------------

ex:SK4_DurabilityDisposition_001 a gdpo:GDPO0000010 ;
    bfo:BFO_0000197 ex:BraunSK4_001 ;
    bfo:BFO_0000054 ex:SK4_EnduranceProcess_001 .

ex:SK4_EnduranceProcess_001 a gdpo:GDPO0000018 ;
    bfo:BFO_0000057 ex:BraunSK4_001 .

ex:iPhone15_DurabilityDisposition_001 a gdpo:GDPO0000010 ;
    bfo:BFO_0000197 ex:iPhone15_001 .
