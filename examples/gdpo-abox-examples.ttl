@prefix bfo: <http://purl.obolibrary.org/obo/> .
@prefix cco: <https://www.commoncoreontologies.org/> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix gdpo: <https://www.ramsprinciplesofgooddesign.com/> .
@prefix gdpoex: <https://www.ramsprinciplesofgooddesign.com/examples/> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

#################################################################
# GDPO ABox Example Snippets
#
# These examples are designed to satisfy the companion SHACL rules
# in gdpo-shapes.ttl, while keeping the ABox minimal.
#
# Replace gdpoex:* IRIs with your project-specific identifiers.
#################################################################

############################
# 1) Ground the 10 canonical Rams statement individuals
############################

# A single material bearer can ground multiple statements (e.g., one
# webpage, book page, poster, or archived PDF containing the set).

gdpoex:RamsTenPrinciplesSource_001
    a bfo:BFO_0000040 ;
    rdfs:label "Material bearer for Rams' Ten Principles text (example)"@en ;
    dcterms:source "Replace with your chosen citation / archive / book / webpage"@en .

# Link each canonical statement individual to at least one bearer.
# (These 10 individuals already exist in GDPO.)

gdpo:GDPO0000066 gdpo:GDPO0000464 gdpoex:RamsTenPrinciplesSource_001 .
gdpo:GDPO0000067 gdpo:GDPO0000464 gdpoex:RamsTenPrinciplesSource_001 .
gdpo:GDPO0000068 gdpo:GDPO0000464 gdpoex:RamsTenPrinciplesSource_001 .
gdpo:GDPO0000069 gdpo:GDPO0000464 gdpoex:RamsTenPrinciplesSource_001 .
gdpo:GDPO0000070 gdpo:GDPO0000464 gdpoex:RamsTenPrinciplesSource_001 .
gdpo:GDPO0000071 gdpo:GDPO0000464 gdpoex:RamsTenPrinciplesSource_001 .
gdpo:GDPO0000072 gdpo:GDPO0000464 gdpoex:RamsTenPrinciplesSource_001 .
gdpo:GDPO0000073 gdpo:GDPO0000464 gdpoex:RamsTenPrinciplesSource_001 .
gdpo:GDPO0000074 gdpo:GDPO0000464 gdpoex:RamsTenPrinciplesSource_001 .
gdpo:GDPO0000075 gdpo:GDPO0000464 gdpoex:RamsTenPrinciplesSource_001 .

############################
# 2) Minimal evaluation record with reified score component + scale
############################

# The artifact being evaluated

gdpoex:Artifact_001
    a bfo:BFO_0000040 ;
    rdfs:label "Example artifact under evaluation"@en .

# The evaluator (agent)

gdpoex:Evaluator_001
    a cco:ont00001017 ;
    rdfs:label "Example evaluator"@en .

# Optional: a method specification (rubric/protocol)

gdpoex:Method_RamsRubric_v1
    a gdpo:GDPO0000052 ;
    rdfs:label "Rams conformance rubric v1 (example)"@en ;
    gdpo:GDPO0000054 gdpo:GDPO0000072 ;  # operationalizes principle (durability)
    skos:definition "Example rubric used to assess an artifact against Rams' durability principle."@en .

# A scale specification explaining what the numeric score means

gdpoex:Scale_0to1_Normalized
    a gdpo:GDPO0000461 ;
    rdfs:label "0â€“1 normalized ratio scale"@en ;
    skos:definition "A normalized ratio scale where 0.0 = worst observed/allowed and 1.0 = best observed/allowed under the specified method."@en .

# A reified score component that ties: value + scale + criterion principle

gdpoex:EvalScore_Durability_001
    a gdpo:GDPO0000456 ;
    rdfs:label "Durability score component (example)"@en ;
    gdpo:GDPO0000458 gdpo:GDPO0000072 ;            # score for principle
    gdpo:GDPO0000460 gdpoex:Scale_0to1_Normalized ; # has score scale
    gdpo:GDPO0000459 "0.92"^^xsd:decimal .          # score value

# Optional: a material bearer for the evaluation record (e.g., PDF report)

gdpoex:EvaluationReportBearer_001
    a bfo:BFO_0000040 ;
    rdfs:label "Material bearer of evaluation report 001 (example)"@en ;
    dcterms:source "Replace with your report file reference / repository identifier"@en .

# The evaluation record (ICE)

gdpoex:EvaluationRecord_001
    a gdpo:GDPO0000044 ;
    rdfs:label "Design evaluation record 001 (example)"@en ;
    gdpo:GDPO0000045 gdpoex:Artifact_001 ;
    gdpo:GDPO0000046 gdpo:GDPO0000072 ;
    gdpo:GDPO0000050 "2026-01-16T12:00:00Z"^^xsd:dateTime ;
    gdpo:GDPO0000048 gdpoex:Method_RamsRubric_v1 ;
    gdpo:GDPO0000457 gdpoex:EvalScore_Durability_001 ;
    gdpo:GDPO0000464 gdpoex:EvaluationReportBearer_001 .

# The producing evaluation process (occurrent) and its participants

gdpoex:EvaluationProcess_001
    a gdpo:GDPO0000055 ;
    rdfs:label "Design evaluation process 001 (example)"@en ;
    gdpo:GDPO0000051 gdpoex:Evaluator_001 ;
    gdpo:GDPO0000465 gdpoex:Artifact_001 ;
    gdpo:GDPO0000463 gdpoex:EvaluationRecord_001 .

# Link the record back to its producing process (recommended by SHACL)

gdpoex:EvaluationRecord_001
    gdpo:GDPO0000462 gdpoex:EvaluationProcess_001 .
