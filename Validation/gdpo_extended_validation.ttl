@prefix ex:    <http://example.org/gdpo-extended-validation/> .
@prefix gdpo:  <https://www.ramsprinciplesofgooddesign.com/> .
@prefix bfo:   <http://purl.obolibrary.org/obo/> .
@prefix cco:   <https://www.commoncoreontologies.org/> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .

#####################################################################
# Base toy ABox (intended-to-PASS)
#####################################################################

### Evaluated artifact (material entity)
ex:ToyArtifact_001 a bfo:BFO_0000040 ;
  rdfs:label "Toy evaluated artifact 001"@en .

### Communication content entity (manual content)
ex:ToyManualContent_001 a gdpo:GDPO0000447 ;
  rdfs:label "Toy user manual content 001"@en .

### Assessment time (BFO temporal region; using 'temporal instant')
ex:T_2026_01_22 a bfo:BFO_0000203 ;
  rdfs:label "2026-01-22T00:00:00Z"@en .

### Score scale specification (reified)
ex:Scale_0to1_Normalized a gdpo:GDPO0000461 ;
  rdfs:label "Normalized 0–1 scale (higher is better)"@en .

### Evaluation method specification that operationalizes a principle TOKEN (canonical honesty statement)
ex:ManualHonestyChecklist_v1 a gdpo:GDPO0000052 ;
  rdfs:label "Manual honesty checklist v1"@en ;
  gdpo:GDPO0000054 gdpo:GDPO0000071 .

### Score component (per-criterion)
ex:Score_Toy_ManualHonesty_001 a gdpo:GDPO0000456 ;
  rdfs:label "Toy manual honesty score (0–1)"@en ;
  gdpo:GDPO0000458 gdpo:GDPO0000071 ;
  gdpo:GDPO0000460 ex:Scale_0to1_Normalized ;
  gdpo:GDPO0000459 "0.93"^^xsd:decimal .

### Evaluation record (criterion is honesty token; also about communication content)
ex:Eval_Toy_ManualHonesty_001 a gdpo:GDPO0000044 ;
  rdfs:label "Toy manual honesty evaluation record (2026-01-22)"@en ;
  gdpo:GDPO0000045 ex:ToyArtifact_001 ;
  gdpo:GDPO0000046 gdpo:GDPO0000071 ;
  gdpo:GDPO0000048 ex:ManualHonestyChecklist_v1 ;
  gdpo:GDPO0000450 ex:ToyManualContent_001 ;
  gdpo:GDPO0000468 ex:T_2026_01_22 ;
  gdpo:GDPO0000050 "2026-01-22T00:00:00Z"^^xsd:dateTime ;
  gdpo:GDPO0000457 ex:Score_Toy_ManualHonesty_001 .

#####################################################################
# Property-chain demonstration (principle -> prescription component -> prescribes)
#####################################################################

### A process token (instance of a GDPO design process type)
ex:ToyDesignCommProcess_001 a gdpo:GDPO0000053 ;
  rdfs:label "Toy design communication process token 001"@en .

### A prescription component token that explicitly prescribes a process token
ex:ToyHonestyPrescriptionComponent_001 a gdpo:GDPO0000061 ;
  rdfs:label "Toy honesty prescription component 001"@en ;
  cco:ont00001942 ex:ToyDesignCommProcess_001 .

### Reuse the canonical honesty statement token as the principle token; add our component
gdpo:GDPO0000071 gdpo:GDPO0000062 ex:ToyHonestyPrescriptionComponent_001 .

#####################################################################
# Contrast case (existential): someValuesFrom can create a phantom target instance
#####################################################################

ex:NaivePrincipleCategory a owl:Class ;
  rdfs:label "Naive principle category (existential contrast)"@en ;
  rdfs:subClassOf gdpo:GDPO0000003 ,
    [ a owl:Restriction ;
      owl:onProperty gdpo:GDPO0000454 ;
      owl:someValuesFrom gdpo:GDPO0000004
    ] .

ex:NaivePrincipleToken_001 a ex:NaivePrincipleCategory ;
  rdfs:label "Naive principle token 001"@en .
