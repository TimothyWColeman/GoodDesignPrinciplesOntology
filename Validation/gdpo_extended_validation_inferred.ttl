    rdfs:label "used in evaluation"@en ;
    rdfs:subPropertyOf cco:ont00001801 ;
    skos:definition "Relates an evaluation method specification to the evaluations that have used it."@en .

gdpo:GDPO0000447 a owl:Class ;
    rdfs:label "product manual content"@en ;
    rdfs:subClassOf gdpo:GDPO0000446 ;
    skos:definition "A design communication content entity intended to instruct users in use and maintenance of a designed artifact."@en ;
    skos:prefLabel "product manual content"@en .

gdpo:GDPO0000450 a owl:ObjectProperty ;
    rdfs:label "is about communication content"@en ;
    rdfs:domain gdpo:GDPO0000044 ;
    rdfs:range gdpo:GDPO0000446 ;
    rdfs:subPropertyOf cco:ont00001808 ;
    skos:definition "Relates a design evaluation record to the communication content (manual/label/ad content) being assessed."@en ;
    skos:prefLabel "is about communication content"@en .

gdpo:GDPO0000452 a owl:Class ;
    rdfs:label "design communicative honesty evaluation"@en ;
    rdfs:subClassOf gdpo:GDPO0000044 ;
    owl:equivalentClass [ a owl:Class ;
            owl:intersectionOf ( gdpo:GDPO0000044 [ a owl:Restriction ;
                        owl:onProperty gdpo:GDPO0000046 ;
                        owl:someValuesFrom gdpo:GDPO0000025 ] [ a owl:Restriction ;
                        owl:onProperty gdpo:GDPO0000450 ;
                        owl:someValuesFrom gdpo:GDPO0000446 ] ) ] ;
    skos:altLabel "communicative honesty evaluation"@en ;
    skos:definition "A design evaluation that assesses whether a communication artifact honestly represents a designed artifact's actual properties and performance."@en ;
    skos:editorialNote "Replaces the deprecated disposition model. Link to the product via 'is about' and to manuals, labels, ads via 'is about communication content'."@en ;
    skos:prefLabel "design communicative honesty evaluation"@en .

gdpo:GDPO0000457 a owl:ObjectProperty ;
    rdfs:label "has score component"@en ;
    rdfs:domain gdpo:GDPO0000044 ;
    rdfs:range gdpo:GDPO0000456 ;
    skos:definition "Relates a design evaluation record to a reified score component that records a score value and its interpretation context (e.g., criterion and scale)."@en ;
    skos:editorialNote "Preferred over attaching multiple gdpo:has score literals directly to the evaluation record when doing multi-criterion evaluation."@en ;
    skos:prefLabel "has score component"@en .

gdpo:GDPO0000468 a owl:ObjectProperty ;
    rdfs:label "assessed during temporal region"@en ;
