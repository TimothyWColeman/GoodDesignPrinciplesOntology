gdpo:GDPO0000461 a owl:Class ;
    rdfs:label "evaluation score scale specification"@en ;
    rdfs:subClassOf cco:ont00000958 ;
    skos:definition "An information content entity that specifies how to interpret evaluation score values (e.g., scale type, bounds, units, and meaning)."@en ;
    skos:editorialNote "Examples include: \"0–1 normalized ratio\", \"Likert 1–5\", \"percentage (0–100%)\", or \"ordinal grade A–F\". Use this to prevent misleading comparisons across methods or principles."@en ;
    skos:prefLabel "evaluation score scale specification"@en .

gdpo:GDPO0000463 a owl:ObjectProperty ;
    rdfs:label "has evaluation record output"@en ;
    rdfs:domain gdpo:GDPO0000055 ;
    rdfs:range gdpo:GDPO0000044 ;
    rdfs:subPropertyOf cco:ont00001986 ;
    skos:definition "Relates a design evaluation process to the design evaluation record it produces as an output."@en ;
    skos:editorialNote "Use this GDPO-specific property rather than the more generic imported output relation when you want explicit domain/range and clear linkage for truthmaking and provenance."@en ;
    skos:prefLabel "has evaluation record output"@en .

gdpo:GDPO0000017 a owl:Class ;
    rdfs:label "design interaction process"@en ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty bfo:BFO_0000057 ;
