gdpo:GDPO0000014 a owl:Class ;
    rdfs:label "design innovation process"@en ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty bfo:BFO_0000057 ;
            owl:someValuesFrom bfo:BFO_0000040 ],
        [ a owl:Restriction ;
            owl:onProperty bfo:BFO_0000057 ;
            owl:someValuesFrom cco:ont00001017 ],
        bfo:BFO_0000015 ;
    skos:altLabel "innovation-in-design process"@en,
        "product innovation process"@en ;
    skos:definition "A process that produces novel or non-trivial changes to a designed artifact’s features or functions relative to an appropriate prior state or comparison class."@en ;
    skos:editorialNote "Keep distinct from project management milestones; the focus is the technical/design change. Align the comparison class (e.g., prior model, category baseline) in annotations rather than in the definition."@en ;
    skos:prefLabel "design innovation process"@en ;
    skos:scopeNote "Typical evidence: invention disclosures, patentable novelty assessments, design reviews showing substantial change."@en .

gdpo:GDPO0000030 a owl:Class ;
    rdfs:label "design aesthetic development process"@en ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty bfo:BFO_0000057 ;
            owl:someValuesFrom bfo:BFO_0000040 ],
        [ a owl:Restriction ;
            owl:onProperty bfo:BFO_0000057 ;
            owl:someValuesFrom cco:ont00001017 ],
        bfo:BFO_0000015 ;
    skos:definition "A process in which an agent develops or refines the form, proportion, harmony, and material finish of a designed artifact."@en ;
    skos:prefLabel "design aesthetic development process"@en ;
    skos:scopeNote "Guides perceptual coherence and appeal without committing to a particular style."@en .

gdpo:GDPO0000031 a owl:Class ;
    rdfs:label "design contextual integration process"@en ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty bfo:BFO_0000057 ;
            owl:someValuesFrom bfo:BFO_0000040 ],
        [ a owl:Restriction ;
            owl:onProperty bfo:BFO_0000057 ;
            owl:someValuesFrom cco:ont00001017 ],
        bfo:BFO_0000015 ;
    skos:altLabel "design environmental integration process"@en ;
    skos:definition "A process that integrates visual and signal salience of the artifact with its setting."@en ;
    skos:editorialNote "Distinguish from minimalism; unobtrusive ≠ austere when signaling (e.g., safety) is required."@en,
        "Renamed from 'design environmental integration process' to reduce confusion with ecological 'environmental friendliness'; this process is about contextual fit and perceptual salience."@en ;
    skos:prefLabel "design contextual integration process"@en ;
    skos:scopeNote "Here 'environment' means the artifact's usage setting/context (e.g., visual environment), not ecological sustainability. Use design environmental friendliness for ecological impact across lifecycle."@en .

gdpo:GDPO0000032 a owl:Class ;
    rdfs:label "design thoroughness assurance process"@en ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty bfo:BFO_0000057 ;
            owl:someValuesFrom bfo:BFO_0000040 ],
        [ a owl:Restriction ;
            owl:onProperty bfo:BFO_0000057 ;
            owl:someValuesFrom cco:ont00001017 ],
        bfo:BFO_0000015 ;
    skos:definition "A process that ensures completeness and fit-and-finish across an artifact's parts and ecosystem, including packaging, documentation, and accessories."@en ;
    skos:prefLabel "design thoroughness assurance process"@en ;
    skos:scopeNote "Ensures attention 'down to the last detail' across all system components."@en .

gdpo:GDPO0000033 a owl:Class ;
    rdfs:label "design reduction process"@en ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty bfo:BFO_0000057 ;
            owl:someValuesFrom bfo:BFO_0000040 ],
        [ a owl:Restriction ;
            owl:onProperty bfo:BFO_0000057 ;
            owl:someValuesFrom cco:ont00001017 ],
        bfo:BFO_0000015 ;
    skos:definition "A process that eliminates superfluous elements to achieve sufficiency with minimal complexity."@en ;
    skos:editorialNote "Distinguish from impoverished function; allow justified exceptions for safety/accessibility cues."@en ;
    skos:prefLabel "design reduction process"@en .

gdpo:GDPO0000034 a owl:Class ;
    rdfs:label "rams ten principles specification"@en ;
    dcterms:creator "Dieter Rams"@en ;
    rdfs:comment "Modeling note: In v1.5.3, the compositional structure of this specification is represented using the GDPO-specific relation has principle component. This avoids treating ICE composition as BFO continuant parthood for GDCs."@en ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty gdpo:GDPO0000060 ;
            owl:someValuesFrom gdpo:GDPO0000020 ],
        [ a owl:Restriction ;
            owl:onProperty gdpo:GDPO0000060 ;
            owl:someValuesFrom gdpo:GDPO0000021 ],
        [ a owl:Restriction ;
            owl:onProperty gdpo:GDPO0000060 ;
            owl:someValuesFrom gdpo:GDPO0000022 ],
        [ a owl:Restriction ;
            owl:onProperty gdpo:GDPO0000060 ;
            owl:someValuesFrom gdpo:GDPO0000023 ],
        [ a owl:Restriction ;
            owl:onProperty gdpo:GDPO0000060 ;
            owl:someValuesFrom gdpo:GDPO0000024 ],
        [ a owl:Restriction ;
            owl:onProperty gdpo:GDPO0000060 ;
            owl:someValuesFrom gdpo:GDPO0000025 ],
        [ a owl:Restriction ;
            owl:onProperty gdpo:GDPO0000060 ;
            owl:someValuesFrom gdpo:GDPO0000026 ],
        [ a owl:Restriction ;
            owl:onProperty gdpo:GDPO0000060 ;
            owl:someValuesFrom gdpo:GDPO0000027 ],
        [ a owl:Restriction ;
            owl:onProperty gdpo:GDPO0000060 ;
            owl:someValuesFrom gdpo:GDPO0000028 ],
        [ a owl:Restriction ;
            owl:onProperty gdpo:GDPO0000060 ;
            owl:someValuesFrom gdpo:GDPO0000029 ],
        [ a owl:Restriction ;
            owl:allValuesFrom [ a owl:Class ;
                    owl:unionOf ( gdpo:GDPO0000020 gdpo:GDPO0000021 gdpo:GDPO0000022 gdpo:GDPO0000023 gdpo:GDPO0000024 gdpo:GDPO0000025 gdpo:GDPO0000026 gdpo:GDPO0000027 gdpo:GDPO0000028 gdpo:GDPO0000029 ) ] ;
            owl:onProperty gdpo:GDPO0000060 ],
        gdpo:GDPO0000035 ;
    skos:definition "A principle specification that includes, as principle components, each of the ten principles articulated by Dieter Rams (innovativeness, usefulness, aesthetics, understandability, unobtrusiveness, honesty, durability, thoroughness, environmental friendliness, and minimalism)."@en ;
    skos:editorialNote "Treat this as a directive information content entity authored by Dieter Rams in the 1970s. Represent its compositional structure using the GDPO-specific relation has principle component, preferably by asserting a specification individual (e.g., RamsTenPrinciplesSpecification) that has principle component each of the ten named Rams principle statement individuals. This avoids committing to BFO continuant parthood for GDC/ICE composition. Do not treat this class as logically equivalent to the conjunction of the ten principles for purposes of conformance: Rams’ framework supports graded and context-dependent conformity."@en ;
    skos:prefLabel "rams ten principles specification"@en ;
    skos:scopeNote "This class represents the historically bounded Ten Principles of Good Design articulated by Dieter Rams. It is intended to serve as a reference specification against which design artifacts or practices can be evaluated or annotated. The specification supports reasoning and comparison by providing a coherent set of principles that can be contrasted with alternative frameworks of design evaluation. It should be used when modeling Rams’ philosophy as a complete set, while the individual principle classes should be used when annotating specific features of artifacts or design processes."@en .

gdpo:GDPO0000036 a owl:Class ;
    rdfs:label "design sourcing process"@en ;
    rdfs:subClassOf gdpo:GDPO0000019 ;
    owl:disjointWith gdpo:GDPO0000040 ;
    skos:definition "A design lifecycle stage process in which raw materials, components, or subassemblies are identified, selected, and acquired for use in the production of an artifact."@en ;
    skos:editorialNote "Distinguish sourcing from distribution (outward supply) and from manufacturing (transformation). Capture provenance, supplier roles, and sustainability criteria as annotations or linked processes."@en ;
    skos:prefLabel "design sourcing process"@en ;
    skos:scopeNote "Includes activities such as material selection, supplier engagement, procurement, and logistics prior to manufacturing. Relevant for evaluating environmental friendliness and supply chain ethics."@en .

gdpo:GDPO0000045 a owl:ObjectProperty ;
    rdfs:label "is about evaluated artifact"@en ;
    rdfs:domain gdpo:GDPO0000044 ;
    rdfs:range bfo:BFO_0000040 ;
    rdfs:subPropertyOf cco:ont00001808 ;
    skos:definition "Relates a design evaluation record to the material entity (artifact/system) that is the subject of the assessment."@en ;
    skos:editorialNote "Prefer a single primary target per record; use multiple evaluations if assessing several artifacts."@en ;
    skos:prefLabel "is about evaluated artifact"@en ;
    skos:scopeNote "Use alongside against_principle for principled conformance."@en .

gdpo:GDPO0000047 a owl:DatatypeProperty ;
    rdfs:label "has score"@en ;
    rdfs:domain gdpo:GDPO0000044 ;
    rdfs:range xsd:decimal ;
    skos:definition "Records a numeric score assigned within the evaluation context."@en ;
    skos:editorialNote "Legacy shortcut: records a numeric score directly on the evaluation record. Prefer reified score components (gdpo:has score component → design evaluation score) when multiple principles or multiple scales are involved, so that each score can be linked to a principle criterion and a score scale specification."@en ;
    skos:prefLabel "has score"@en ;
    skos:scopeNote "Optional; qualitative evaluations may omit."@en .

gdpo:GDPO0000054 a owl:ObjectProperty ;
    rdfs:label "operationalizes principle"@en ;
    rdfs:domain gdpo:GDPO0000052 ;
    rdfs:range gdpo:GDPO0000003 ;
    rdfs:subPropertyOf cco:ont00001808 ;
    skos:definition "Links an evaluation method to the design principle(s) it measures."@en ;
    skos:prefLabel "operationalizes principle"@en .

gdpo:GDPO0000057 a owl:Class ;
    rdfs:label "design material honesty basis"@en ;
    rdfs:subClassOf gdpo:GDPO0000056 ;
    skos:definition "A design honesty basis that grounds a design material honesty disposition by aligning material-related cues (e.g., finish, texture, visible structure) with the bearer's actual material composition."@en .

